package com.it.beans;

public class Car {
	private IEngine eng;
	
	public Car() {
		System.out.println("Car.Car()");
	}
	
	public void setEng(IEngine eng)
	{
		System.out.println("Car.setEng()");
		this.eng=eng;
	}
	
	public void drive()
	{
		int status=eng.start();
		if(status>=1)
			System.out.println("Journey started");
		else 
			System.out.println("Engine trouble");
	}
}
